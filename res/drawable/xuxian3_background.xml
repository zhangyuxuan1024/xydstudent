<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape>
            <stroke
                android:width="2dp"
                android:color="#000000" />

            <solid android:color="#FFFFFF" />

            <corners android:radius="5dp" />
        </shape>
    </item>
    <item
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp">
        <shape>
            <stroke
                android:dashWidth="10dp"
                android:dashGap="10dp"
                android:width="2dp"
                android:color="#000000" />

            <solid android:color="#FFFFFF" />

            <corners android:radius="5dp" />
        </shape>
    </item>
    <item
        android:bottom="8dp"
        android:left="8dp"
        android:right="8dp"
        android:top="8dp">
        <shape>
            <stroke
                android:width="2dp"
                android:color="#000000" />

            <solid android:color="#FFFFFF" />

            <corners android:radius="5dp" />
        </shape>
    </item>

</layer-list>